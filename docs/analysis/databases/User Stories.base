views:
  - type: table
    name: Table
    filters:
      and:
        - file.inFolder("The-Meridian-Grid/docs/analysis/UserStories")
        - moscow == "MUST"
    order:
      - file.name
      - actor
      - status
      - title
      - moscow
      - justification
    sort:
      - property: file.name
        direction: ASC
    columnSize:
      file.name: 138
      note.title: 120
